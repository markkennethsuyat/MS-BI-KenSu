        <Field Name="ReportFolder">
          <DataField>ReportFolder</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="OrderNumber">
          <DataField>OrderNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
