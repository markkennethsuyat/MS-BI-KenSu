    <Variable Name="FormatDate">
      <Value>dd-MMM-yyyy</Value>
    </Variable>
    <Variable Name="FormatDateTime">
      <Value>dd-MMM-yyyy  hh:mm tt</Value>
    </Variable>
    <Variable Name="ReportExecutionTime">
      <Value>
        =  Right("00" + (System.DateTime.Now.Subtract(Globals!ExecutionTime).Hours).ToString, 2) + ":"
        + Right("00" + (System.DateTime.Now.Subtract(Globals!ExecutionTime).Minutes).ToString, 2) + ":"
        + Right("00" + (System.DateTime.Now.Subtract(Globals!ExecutionTime).Seconds).ToString, 2)
      </Value>
    </Variable>
	<Variable Name="ReportServer">
      <Value>=Replace(Globals!ReportServerUrl, "/ReportServer", "")</Value>
    </Variable>
    <Variable Name="ReportRunDateTime">
      <Value>=Format(DateTime.Now, Variables!FormatDateTime.Value)</Value>
    </Variable>
    <Variable Name="UrlReport">
      <Value>
        =Variables!ReportServer.Value + "/ReportServer?"
        + Replace(Globals!ReportFolder, " ", "+") + "%2f"
        + Replace(Globals!ReportName, " ", "+") + "&amp;rs:Command=Render"
      </Value>
    </Variable>
    <Variable Name="UrlReportWithParameters">
      <Value>=Variables!UrlReport.Value
 + "&amp;ReportFolder=" + Join(Parameters!ReportFolder.Value, "&amp;ReportFolder=")
 + "&amp;SearchType=" + Join(Parameters!SearchType.Value, "&amp;SearchType=")
 + IIf(IsNothing(Parameters!SearchFor.Value), "&amp;SearchFor:IsNull=True", "&amp;SearchFor=" + Parameters!SearchFor.Value)
 + IIf(IsNothing(Parameters!CreatedDate.Value), "&amp;CreatedDate:IsNull=True", "&amp;CreatedDate=" + Format(Parameters!CreatedDate.Value, Variables!FormatDate.Value))
 + IIf(IsNothing(Parameters!ModifiedDate.Value), "&amp;ModifiedDate:IsNull=True", "&amp;ModifiedDate=" + Format(Parameters!ModifiedDate.Value, Variables!FormatDate.Value))
 + IIf(IsNothing(Parameters!CreatedBy.Value), "&amp;CreatedBy:IsNull=True", "&amp;CreatedBy=" + Parameters!CreatedBy.Value)
 + IIf(IsNothing(Parameters!ModifiedBy.Value), "&amp;ModifiedBy:IsNull=True", "&amp;ModifiedBy=" + Parameters!ModifiedBy.Value)
 + "&amp;ShowSql=" + CStr(Parameters!ShowSql.Value)</Value>
    </Variable>
    <Variable Name="UserName">
      <Value>=Right(User!UserID, len(User!UserID)-instr(User!UserID, "\"))</Value>
    </Variable>
  </Variables>
