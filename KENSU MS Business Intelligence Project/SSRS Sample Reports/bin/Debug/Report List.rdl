    <DataSet Name="ReportFolders">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	[ReportFolder] = SUBSTRING(c.[Path], 2, Len(c.[Path])-Len(c.[Name])-2)
	, [OrderNumber] = 2
FROM 
	[ReportServer].[dbo].[Catalog] AS c WITH (NOLOCK) 
WHERE
	1=1
	AND c.[Type] = 2
UNION 
SELECT @all_value, 1
ORDER BY 2, 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
