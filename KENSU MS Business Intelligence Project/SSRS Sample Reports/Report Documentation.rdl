    <DataSet Name="ReportNames">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@folder_name">
            <Value>=Parameters!folder_name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT    
	report_name = cat.[Name] 
FROM
	ReportServer.dbo.Catalog AS cat
WHERE 
	1=1
	AND cat.[Type] = 2
	AND (@all_value IN (@folder_name) OR SUBSTRING(cat.[Path], 2, Len(cat.[Path])-Len(cat.[Name])-2) IN(@folder_name)) </CommandText>
      </Query>
      <Fields>
        <Field Name="report_name">
          <DataField>report_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
