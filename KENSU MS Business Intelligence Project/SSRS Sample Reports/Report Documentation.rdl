  <DataSets>
    <DataSet Name="Folders">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH 
catalog_type_description
AS
(
	SELECT tbl.* FROM (VALUES
	  ( 1, 'Folder')
	, ( 2, 'Report')
	, ( 3, 'Resource')
	, ( 4, 'Linked Report')
	, ( 5, 'Data Source')
	, ( 6, 'Report Model')
	, ( 8, 'Shared Dataset')
	, ( 9, 'Report Part')
	) tbl ([TypeID], [TypeDescription]) 
	WHERE 
		1=1
		AND TypeID = 2
)
SELECT 
	folder_name = SUBSTRING(cat.Path, 2, Len(cat.Path)-Len(cat.Name)-2)   
FROM 
	ReportServer.dbo.Catalog AS cat WITH (NOLOCK) 
	INNER JOIN catalog_type_description AS ctd ON ctd.TypeID = cat.[Type]
UNION 
SELECT @all_value
ORDER BY 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="folder_name">
          <DataField>folder_name</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Reports">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@report_name">
            <Value>=Parameters!report_name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH
report_users 
AS
(
	SELECT 
		  UserID
		, user_name_format = UPPER(RIGHT(UserName, (LEN(UserName)-CHARINDEX('\', UserName)))) 
	FROM 
		ReportServer.dbo.Users
)
SELECT    
	  cat.ItemID
	, report_name = cat.[Name] 
	, report_description = cat.[Description]
	, report_parameter = cat.Parameter
	, report_creation_id = cat.CreatedById
	, report_modified_id = cat.ModifiedById
	, report_created_by = urc.user_name_format
	, report_modified_by = urm.user_name_format
	, report_creation_date = cat.CreationDate 
	, report_modified_date = cat.ModifiedDate 
	, report_folder = SUBSTRING(cat.[Path], 2, Len(cat.[Path])-Len(cat.[Name])-2) 
	, report_path = cat.[Path]
	, url_report_folder = 'http://' + Host_Name() + '/Reports/Pages/Folder.aspx?ItemPath=%2f' + SUBSTRING(cat.[Path], 2, Len(cat.[Path])-Len(cat.[Name])-2) + '&amp;ViewMode=List'
	, url_report = 'http://' + Host_Name() + '/Reports/Pages/Report.aspx?ItemPath=%2f' + SUBSTRING(cat.[Path], 2, Len(cat.[Path])-Len(cat.[Name])-2) + '%2f' + cat.[Name] 
	--, report_definition = CONVERT(VARCHAR(MAX),CONVERT(VARBINARY(MAX), cat.content))  
FROM 
	ReportServer.dbo.Catalog AS cat
	LEFT OUTER JOIN report_users AS urc ON cat.CreatedById = urc.UserID 
	LEFT OUTER JOIN report_users AS urm ON cat.ModifiedById = urm.UserID 
WHERE 
	1=1
	AND cat.[Type] = 2
	AND cat.[Name] IN(@report_name)</CommandText>
      </Query>
      <Fields>
